@model IEnumerable<sphinxsolaries.Caems.Models.CAEMS_Project3>
 
 
<div class="card">
    <div class="card-body">
        <h4 class="card-title">View Projects</h4> 
        <div class="table-responsive">
            <table class="table table-striped table-bordered zero-configuration">
                <thead>
                    <tr> 
                        <th>Service Company</th>
                        <th>Organization</th>
                        <th>Project Title</th>
 
                    </tr> 
                </thead>
                <tbody>
                    @foreach (var item in Model)
                    {
                        <tr>      
                            <td>@Html.DisplayFor(modelItem => item.Service_Company)</td>
                            <td>@Html.DisplayFor(modelItem => item.Organization)</td>
                           <td>@Html.DisplayFor(modelItem => item.Project_title)</td>
                        </tr>
                    }
                    
                </tbody>
                <tfoot>
                    <tr>             
                        <th>Service Company</th>
                        <th>Organization</th>
                        <th>Project Title</th>
 
                    </tr>
                </tfoot>
            </table>
        </div>
    </div>
</div> 
<script> 
    function msg(txt) {
        sweetAlert("...", txt, "info");
    }
    var statusMessage = "@ViewBag.status" || false; 
    if (statusMessage != false && statusMessage.length > 0) {
        msg(statusMessage); 
    }
</script>