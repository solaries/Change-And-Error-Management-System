@model IEnumerable<sphinxsolaries.Caems.Models.CAEMS_Project2>
 
 
<div class="card">
    <div class="card-body">
        <h4 class="card-title">View Project</h4> 
        <div class="table-responsive">
            <table class="table table-striped table-bordered zero-configuration">
                <thead>
                    <tr>
                        <th>   </th> 
                        <th>Project Title</th>
                        <th>Organization</th>
 
                    </tr> 
                </thead>
                <tbody>
                    @foreach (var item in Model)
                    {
                        <tr>                            
                            <td>@Html.ActionLink("Select", "edit_Project", new
                           {
                            id=item.Id,
                            Organization = item.Organization 
                            ,Project_Title = item.Project_title
                               })</td> 
                           <td>@Html.DisplayFor(modelItem => item.Project_title)</td>
                           <td>@Html.DisplayFor(modelItem => item.Organization2[0].Name)</td> 
                        </tr>
                    }
                    
                </tbody>
                <tfoot>
                    <tr>                        
                        <th>   </th> 
                        <th>Project Title</th>
                        <th>Organization</th> 
 
                    </tr>
                </tfoot>
            </table>
        </div>
    </div>
</div> 
<script> 
    function msg(txt) {
        sweetAlert("...", txt, "info");
    }
    var statusMessage = "@ViewBag.status" || false; 
    if (statusMessage != false && statusMessage.length > 0) {
        msg(statusMessage); 
    }
</script>