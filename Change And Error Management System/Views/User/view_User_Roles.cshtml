@model IEnumerable<sphinxsolaries.Caems.Data.Models.CAEMS_role_User>
 
 
<div class="card">
    <div class="card-body">
        <h4 class="card-title">View User Roles</h4> 
        <div class="table-responsive">
            <table class="table table-striped table-bordered zero-configuration">
                <thead>
                    <tr>
                        <th>   </th> 
                        <th>roleName</th>
                        @*<th>client</th>*@
 
                    </tr> 
                </thead>
                <tbody>
                    @foreach (var item in Model)
                    {
                        <tr>                            
                            <td>@Html.ActionLink("Select", "edit_User_Roles", new
                           {
                            id=item.Id
                            ,roleName = item.Rolename
                               })</td> 
                           <td>@Html.DisplayFor(modelItem => item.Rolename)</td>
                           @*<td>@Html.DisplayFor(modelItem => item.Client)</td>*@
                        </tr>
                    }
                    
                </tbody>
                <tfoot>
                    <tr>                        
                        <th>   </th> 
                        <th>roleName</th>
                        @*<th>client</th>*@
 
                    </tr>
                </tfoot>
            </table>
        </div>
    </div>
</div> 
<script> 
    function msg(txt) {
        sweetAlert("...", txt, "info");
    }
    var statusMessage = "@ViewBag.status" || false; 
    if (statusMessage != false && statusMessage.length > 0) {
        msg(statusMessage); 
    }
</script>